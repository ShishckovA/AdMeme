{
  "manifest_version": 2,
  "name": "AdMeme",
  "version": "1.1",
  
  "icons": {
    "128": "icon.png"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["onpageload.js"]}
  ],

  "permissions": [
    "<all_urls>"
  ],
   "web_accessible_resources": [
    "memes/*.jpg"
    ],
  "browser_action": {
    "default_title": "AdMeme",
    "default_icon": "icon.png",
    "default_popup": "popUp.html"
  }
}
