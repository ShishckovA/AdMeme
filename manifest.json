 {
  "manifest_version": 2,
  "name": "AdMeme",
  "version": "1.0",
  
  "icons": {
    "128": "icon.png"
  },

  "content_scripts": [ {
    "matches": ["http://*/*"],
    "js": ["onpageload.js"]
  }
  ],
  "background": {
    "js": ["onpageload.js"]
  },

  "permissions": [
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_title": "AdMeme",
    "default_icon": "icon.png"
  }
  
}