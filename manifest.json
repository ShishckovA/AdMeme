
 {
  "manifest_version": 2,
  "name": "Lost Time",
  "version": "1.0",
  
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],

      "browser_action": {
        "default_title": "LostTime",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
  
}